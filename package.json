{
  "name": "lazada-open-platform-sdk",
  "version": "2.0.0-0",
  "description": "Lazada Open Platform SDK for Node.js",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --runInBand --silent=false --colors",
    "flow": "flow check --include-warnings",
    "lint": "eslint . --cache --ext js --fix",
    "lib:transpile": "babel src -d lib --copy-files --ignore '__tests__'",
    "lib:clean": "rm -r lib/* || true",
    "dev": "npm run lib:transpile -- -w",
    "check": "npm run lint && npm run flow && npm run test",
    "build": "NODE_ENV=production npm run lib:clean && npm run lib:transpile",
    "preversion": "npm test",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/branch8/lazada-open-platform-sdk.git"
  },
  "keywords": [
    "lazada",
    "sdk",
    "lazop"
  ],
  "author": "Conrad Lo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/branch8/lazada-open-platform-sdk/issues"
  },
  "homepage": "https://github.com/branch8/lazada-open-platform-sdk#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-prettier": "^2.6.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.74.0",
    "jest": "^23.1.0",
    "prettier": "^1.13.5"
  },
  "dependencies": {
    "flow-runtime": "^0.17.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  }
}
